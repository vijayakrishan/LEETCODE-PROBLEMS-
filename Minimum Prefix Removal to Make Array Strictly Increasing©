class Solution {
public:
    int minimumPrefixLength(vector<int>& nums) {
        int n=nums.size();
        for(int i=n-2;i>=0;i--)
            {
                if(nums[i]>=nums[i+1]){
                    return i+1;
                }
            }
        return 0;
    }
};Â©leetcode

OUTPUT:
Input: nums = [1,-1,2,3,3,4,5]
Output: 4
