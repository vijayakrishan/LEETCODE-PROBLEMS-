class Solution {
public:
    vector<int> numberGame(vector<int>& nums) {
        vector<int>arr;
        sort(nums.begin(),nums.end());
        for(int i=0;i<nums.size();i+=2)
        {
            int j=i+1;
                swap(nums[i],nums[j]);
                arr.push_back(nums[i]);
                arr.push_back(nums[j]);      
         }
         return arr;
    }
};


OUTPUT:
Input: nums = [5,4,2,3]
Output: [3,2,5,4]
