class Solution {
public:
    int singleNumber(vector<int>& nums) {
       map<int,int>ans;
       for(int i=0;i<nums.size();i++)
       {
        ans[nums[i]]++;
       }
       for(auto it:ans)
       {
        if(it.second!=3)
       return it.first;
       } 
       return -1;
    }
};

OUTPUT:
Input: nums = [2,2,3,2]
Output: 3
