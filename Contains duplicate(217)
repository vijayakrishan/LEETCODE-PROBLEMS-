class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        int n=nums.size();
        map<int,int>map;
        for(int i=0;i<n;i++)
        {
            map[nums[i]]++;
            if(map[nums[i]]>1)
            return true;
        }
        return false;
    }
};

OUTPUT:
Input: nums = [1,2,3,1]
Output: true
