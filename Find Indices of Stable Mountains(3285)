class Solution {
public:
    vector<int> stableMountains(vector<int>& height, int threshold) {
        vector<int>stable;
        for(int i=1;i<height.size();i++)
        {
            if(height[i-1]>threshold)
            stable.push_back(i);
        }
        return stable;
    }
};
OUTPUT:
Input: height = [1,2,3,4,5], threshold = 2
Output: [3,4]
