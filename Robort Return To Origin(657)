bool judgeCircle(char* moves) {
    int x = 0, y = 0;

    for (int i = 0; i < strlen(moves); i++) {
        char move = moves[i];
        if (move == 'U') 
        y++;
        else if (move == 'D') 
        y--;
        else if (move == 'L') 
        x--;
        else if (move == 'R') 
        x++;
    }

    if(x==0&&y==0)
    return true;
    else
    return false;
}


OUTPUT:
Input: moves = "UD"
Output: true

Input: moves = "LL"
Output: false
