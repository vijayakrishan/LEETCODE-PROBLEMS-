class Solution {
public:
    int numberOfEmployeesWhoMetTarget(vector<int>& hours, int target) {
        int count=0;
        for(int i=0;i<hours.size();i++)
        {
            if(hours[i]>=target)
            count++;

        }
        return count ;
    }
};


OUTPUT:
Input: hours = [0,1,2,3,4], target = 2
Output: 3
